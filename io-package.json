{
  "common": {
    "name": "chargemaster",
    "version": "0.1.4",
    "title": "chargemaster",
    "titleLang": {
      "en": "ChargeMaster",
      "de": "ChargeMaster",
      "ru": "ChargeMaster",
      "pt": "ChargeMaster",
      "nl": "ChargeMaster",
      "fr": "ChargeMaster",
      "it": "ChargeMaster",
      "es": "ChargeMaster",
      "pl": "ChargeMaster",
      "zh-cn": "ChargeMaster"
    },
    "desc": {
      "en": "Adapter for managing of multi EV-chargers with use of PV-energy",
      "de": "Adapter zur Verwaltung von Multi-EV-Ladegeräten unter Verwendung von PV-Energie",
      "ru": "Адаптер для управления несколькими электромобилями с использованием PV-энергии",
      "pt": "Adaptador para gerenciamento de carregadores multi EV com uso de energia fotovoltaica",
      "nl": "Adapter voor het beheren van meerdere EV-laders met gebruik van PV-energie",
      "fr": "Adaptateur pour la gestion de plusieurs chargeurs EV avec utilisation d'énergie PV",
      "it": "Adattatore per la gestione di più caricatori EV con utilizzo di energia fotovoltaica",
      "es": "Adaptador para la gestión de varios cargadores EV con uso de energía fotovoltaica",
      "pl": "Adapter do zarządzania wieloma ładowarkami EV z wykorzystaniem energii PV",
      "zh-cn": "使用PV能源管理多个EV充电器的适配器"
    },
    "authors": [
      "Christian Hombach"
    ],
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "enabled": true,
    "license": "MIT",
    "loglevel": "info",
    "readme": "https://raw.githubusercontent.com/hombach/ioBroker.chargemaster/master/README.md",
    "icon": "chargemaster.png",
    "extIcon": "https://raw.githubusercontent.com/hombach/ioBroker.chargemaster/master/admin/chargemaster.png",
    "keywords": [ "chargemaster", "wallbox" ],
    "type": "vehicle",
    "connectionType": "local",
    "dataSource": "poll",
    "news": {
      "0.1.4": {
        "en": "bumped dependencies"
      },
      "0.1.3": {
        "en": "fixed vulnerability"
      },
      "0.1.2": {
        "en": "code cleanup and optimization, fixed onStateChange"
      },
      "0.1.1": {
        "en": "fixed errors with js-controller 3.3.x, bumped dependencies"
      },
      "0.1.0": {
        "en": "first running version, fixed to 3 boxes"
      },
      "0.0.7": {
        "en": "added MaxAmpTotal, MinAmpWallBox, MaxAmpWallBox"
      },
      "0.0.6": {
        "en": "added collection and calc of total charge power"
      },
      "0.0.5": {
        "en": "fixed to box 2"
      },
      "0.0.4": {
        "en": "fix error in foreign state popup"
      },
      "0.0.2": {
        "en": "fix errors to get it running in old single wallbox mode"
      },
      "0.0.1": {
        "en": "initial release"
      }
    },
    "main": "main.js",
    "compact": true,
    "materialize": true,
    "materializeTab": true,
    "supportCustoms": false,
    "dependencies": [
      {
        "js-controller": ">=3.0.0",
        "admin": ">=4.0.0"
      }
    ]
  },

  "native": {
    "cycletime": 10000,
    "MaxAmpTotal": 6,
    "StateHomeBatSoc": "",
    "StateHomeSolarPower": "",
    "StateHomePowerConsumption": "",
    "StateWallBox0ChargeCurrent": "",
    "StateWallBox0ChargeAllowed": "",
    "StateWallBox0ChargePower": "",
    "StateWallBox0MeasuredMaxChargeAmp": "",
    "MinAmpWallBox0": 6,
    "MaxAmpWallBox0": 16,
    "StateWallBox1ChargeCurrent": "",
    "StateWallBox1ChargeAllowed": "",
    "StateWallBox1ChargePower": "",
    "StateWallBox1MeasuredMaxChargeAmp": "",
    "MinAmpWallBox1": 6,
    "MaxAmpWallBox1": 16,
    "StateWallBox2ChargeCurrent": "",
    "StateWallBox2ChargeAllowed": "",
    "StateWallBox2ChargePower": "",
    "StateWallBox2MeasuredMaxChargeAmp": "",
    "MinAmpWallBox2": 6,
    "MaxAmpWallBox2": 16
  },
  
  "objects": [],

  "instanceObjects": [
    {
      "_id": "Power",
      "type": "channel",
      "common": { "name": "current charging power data" },
      "native": {}
    },
    {
      "_id": "Power.Charge",
      "type": "state",
      "common": {
        "role": "value.power",
        "name": "actual total charging-power",
        "type": "number",
        "unit": "W",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },


    {
      "_id": "Statistics_Total",
      "type": "channel",
      "common": { "name": "statistical data total lifetime" },
      "native": {}
    },
    {
      "_id": "Statistics_Total.Charged",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Totally charged sum all wallboxes",
        "type": "number",
        "unit": "kWh",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },


    {
      "_id": "Settings",
      "type": "channel",
      "common": { "name": "states to dynamically adjust global settings" },
      "native": {}
    },
    {
      "_id": "Settings.Setpoint_HomeBatSoC",
      "type": "state",
      "common": {
        "role": "level",
        "name": "Setpoint for home battery state of charge",
        "type": "number",
        "unit": "%",
        "read": true,
        "write": true,
        "def": 80
      },
      "native": {}
    },

    {
      "_id": "Settings.WB_0",
      "type": "channel",
      "common": { "name": "states to dynamically adjust wallbox 0 settings" },
      "native": {}
    },
    {
      "_id": "Settings.WB_0.ChargeNOW",
      "type": "state",
      "common": {
        "role": "switch",
        "name": "ChargeNOW enabled for wallbox 0",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_0.ChargeCurrent",
      "type": "state",
      "common": {
        "role": "value.current",
        "name": "Setting charge current output for wallbox 0",
        "type": "number",
        "unit": "A",
        "read": true,
        "write": true,
        "def": 6
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_0.ChargeManager",
      "type": "state",
      "common": {
        "role": "switch.mode.auto",
        "name": "Charge Manager for wallbox 0 enabled",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },

    {
      "_id": "Settings.WB_1",
      "type": "channel",
      "common": { "name": "states to dynamically adjust wallbox 1 settings" },
      "native": {}
    },
    {
      "_id": "Settings.WB_1.ChargeNOW",
      "type": "state",
      "common": {
        "role": "switch",
        "name": "ChargeNOW enabled for wallbox 1",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_1.ChargeCurrent",
      "type": "state",
      "common": {
        "role": "value.current",
        "name": "Setting charge current output for wallbox 1",
        "type": "number",
        "unit": "A",
        "read": true,
        "write": true,
        "def": 6
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_1.ChargeManager",
      "type": "state",
      "common": {
        "role": "switch.mode.auto",
        "name": "Charge Manager for wallbox 1 enabled",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },

    {
      "_id": "Settings.WB_2",
      "type": "channel",
      "common": { "name": "states to dynamically adjust wallbox 2 settings" },
      "native": {}
    },
    {
      "_id": "Settings.WB_2.ChargeNOW",
      "type": "state",
      "common": {
        "role": "switch",
        "name": "ChargeNOW enabled for wallbox 2",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_2.ChargeCurrent",
      "type": "state",
      "common": {
        "role": "value.current",
        "name": "Setting charge current output for wallbox 2",
        "type": "number",
        "unit": "A",
        "read": true,
        "write": true,
        "def": 6
      },
      "native": {}
    },
    {
      "_id": "Settings.WB_2.ChargeManager",
      "type": "state",
      "common": {
        "role": "switch.mode.auto",
        "name": "Charge Manager for wallbox 2 enabled",
